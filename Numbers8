fn main() {
    let mut sum = 0;
    for i in -3..2 {
        sum += i
    }

    assert!(sum == -5);

    for c in 'a'..='z' {
        println!("{}",c);
    }
}




чат гпт предлогает ещё

fn main() {
    let mut sum = 0;
    for i in -3..2 {
        sum += i;
    }

    assert!(sum == -5);

    for c in 'a'..='z' {
        if c == 'a' {
            print!("{}", c as u32);
        } else {
            print!(" - {}", c as u32);
        }
    }
    println!();
}
